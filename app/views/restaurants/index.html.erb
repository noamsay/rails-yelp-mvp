
<h1>My list of restaurants</h1>
<div class="container">
  <div class="row">
     <div class="col-xs-6">
        <% @restaurants.each do |restaurant| %>
          <h2><%= restaurant.name %></h2>
          <p><%= restaurant.address %></p>
          <p><%= restaurant.phone_number %></p>
          <p><%= restaurant.category %></p>
        <% end %>
    </div>
  </div>
</div>
