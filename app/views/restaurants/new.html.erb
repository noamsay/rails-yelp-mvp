<h1>Create a New Restaurant</h1>
<div class="container">
  <div class="row">
     <div class="col-xs-6">
        <%= simple_form_for(@restaurant) do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :phone_number %>
        <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
        <%= f.button :submit, :class => "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>



