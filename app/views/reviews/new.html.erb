<h1>Add a Review</h1>
<div class="container">
  <div class="row">
     <div class="col-xs-6">
        <%= simple_form_for [@restaurant, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating %>
        <%= f.button :submit, :class => "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
